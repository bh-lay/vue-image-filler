{
  "name": "vue-image-filler",
  "version": "1.0.2",
  "description": "图像[填充]剪裁工具 [ Vue Image clipping tool ]",
  "main": "dist/vue-image-filler.umd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bh-lay/vue-image-filler.git"
  },
  "keywords": [
    "vue",
    "image",
    "image",
    "crop",
    "cropper"
  ],
  "author": "剧中人",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bh-lay/vue-image-filler/issues"
  },
  "homepage": "https://github.com/bh-lay/vue-image-filler#readme",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^3.0.0",
    "date-fns": "^1.30.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.5.0",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "serve": "^10.1.2"
  },
  "scripts": {
    "build": "npm run build:es && npm run build:umd",
    "build:es": "rollup -c -f es -o ./dist/vue-image-filler.es.js",
    "build:umd": "rollup -c -f umd -o ./dist/vue-image-filler.umd.js",
    "watch": "rollup -c -w -f umd -o ./dist/vue-image-filler.umd.js",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public"
  }
}
